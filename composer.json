{
  "name": "andrew-svirin/mt942-php",
  "type": "library",
  "description": "PHP library for parse MT942 Swift format. Helper for EBICS.",
  "keywords": [
    "mt942",
    "swift",
    "PHP",
    "normalizer",
    "validator"
  ],
  "authors": [
    {
      "name": "Andrew Svirin"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "AndrewSvirin\\MT942\\": "src"
    }
  },
  "require": {
    "php": "^7.2 || ^8",
    "symfony/validator": "^3.4|^4.0|^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5 | ^9.1",
    "phpstan/phpstan": "^0.12.57",
    "squizlabs/php_codesniffer": "^3.3"
  }
}
